{
  "name": "blank",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@react-renderer/app": "latest",
    "@react-renderer/router": "latest",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "fastify": "^3.14.1",
    "fastify-cookie": "^5.3.0",
    "fastify-static": "^4.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "dotenv -e .env.production node ./server/index",
    "start:dev": "concurrently \"node ./livereload\" \"npm run build:dev\" \"nodemon --exec 'dotenv -e .env.development node ./server/index'\"",
    "build": "dotenv -e .env.production webpack --config ./webpack.config.js",
    "build:dev": "dotenv -e .env.development webpack --config ./webpack.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "build/*"
    ],
    "delay": "1000",
    "extensions": "js,css,scss,json,html,png,svg,jpg,jpeg,webp,bmp,ico,txt"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "dotenv-cli": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "livereload": "^0.9.3",
    "mini-css-extract-plugin": "^1.4.0",
    "nodemon": "^2.0.7",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.30.0",
    "webpack-assets-manifest": "^5.0.1",
    "webpack-cli": "^4.6.0",
    "webpack-nano": "^1.1.1",
    "workbox-webpack-plugin": "^6.1.2"
  }
}
